{
  "version": 2,
  "builds": [{
      "src": "website/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [{
      "src": "/api(.*)|/auth/(logout|login)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ]
}