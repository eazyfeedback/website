{
  "version": 2,
  "alias": ["essayfeedback"],
  "builds": [{
      "src": "website/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [{
      "src": "/api(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ]
}